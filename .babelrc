{
  "presets": [
    // "stage-3",
    [
      "env",
      {
        "targets": {
          "browsers": [ "last 2 versions", "safari >= 8" ],
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "syntax-dynamic-import",
    "transform-vue-jsx",
    ["transform-runtime", {
        "polyfill": false,
        "regenerator": true
    }],
